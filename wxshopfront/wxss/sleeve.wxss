/*统一的图片点击效果css，哪里使用直接import即可*/

/*移动效果*/
.rect-hover{
    position: relative;
    top: 3rpx;
    left: 3rpx;
    box-shadow: 0px 0px 8px rgba(0, 0, 0, .1) inset;
}

/*小缩放效果*/
.small-hover{
    opacity: 0.9;
    transform: scale(0.95,0.95);
}
/*中等缩放效果*/
.medium-hover{
    opacity: 0.8;
    transform: scale(0.85,0.85);
}